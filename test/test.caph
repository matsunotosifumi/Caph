hoge(a, b, c) ~= ((\x y -> x + x + x + y)a b) + c [ a < 5 ]
               = a * b + c                [ b < 5 ]
              ~= a + b * c                [ c < 5 ]
               = piyo(a,b,c) - huga       [ otherwise ]
               where piyo(l, m, n) = l + m + n
                     huga = (\e f g -> e + f + g)a b c


